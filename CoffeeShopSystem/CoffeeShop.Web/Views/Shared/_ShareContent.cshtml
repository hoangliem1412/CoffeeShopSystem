<script src="~/scripts/jquery-1.10.2.min.js"></script>
<script>
    $(document).ready(function () {
        //event click Thêm mới
        $("#btnAddNew").click(function () {
            $("#form")[0].reset();
            $("#MyModal").modal();
        });

        //event click delete
        $('.btn-danger').click(function () {
            $("#DeleteConfirmation").modal("show");
        });
    });
</script>